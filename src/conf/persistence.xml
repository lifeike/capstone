<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="CPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>models.Orders</class>
    <class>models.User</class>
    <class>models.Roles</class>
    <class>models.Category</class>
    <class>models.Provinces</class>
    <class>models.OrderItems</class>
    <class>models.Items</class>
    <class>models.OrderItem</class>
    <class>models.Item</class>
    <class>models.Province</class>
    <class>models.Role</class>
    <class>models.OrderStatus</class>
    <class>models.Provinces_1</class>
    <class>models.Roles_1</class>
    <class>models.Contact</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/capstonedb?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="password"/>
    </properties>
  </persistence-unit>
</persistence>
